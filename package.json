{
  "name": "yarn-why-duplicate",
  "version": "2.0.0",
  "description": "Simple bin to know why a package is duplicated",
  "keywords": [
    "yarn",
    "berry"
  ],
  "author": "Christophe Hurpeau <christophe@hurpeau.com> (https://christophe.hurpeau.com)",
  "license": "ISC",
  "repository": "https://github.com/christophehurpeau/yarn-why-duplicate.git",
  "homepage": "https://github.com/christophehurpeau/yarn-why-duplicate#readme",
  "bugs": {
    "url": "https://github.com/christophehurpeau/yarn-why-duplicate/issues"
  },
  "type": "module",
  "packageManager": "yarn@4.0.2",
  "engines": {
    "node": ">=18.12.0"
  },
  "sideEffects": false,
  "bin": "./bin/yarn-why-duplicate.mjs",
  "main": "./lib/index.js",
  "exports": {
    ".": "./lib/index.js",
    "./package.json": "./package.json"
  },
  "files": [
    "lib",
    "bin"
  ],
  "scripts": {
    "checks": "node scripts/check-package.js",
    "lint": "yarn run lint:prettier && yarn run lint:eslint",
    "lint:eslint": "eslint --report-unused-disable-directives --resolve-plugins-relative-to . --quiet *.{js,cjs,mjs} lib bin scripts",
    "lint:prettier": "pob-root-prettier --check .",
    "lint:prettier:fix": "pob-root-prettier --write .",
    "postinstallDev": "pob-root-postinstall",
    "test": "TS_NODE_PROJECT=tsconfig.test.json node --test test/*.js",
    "test:coverage": "TS_NODE_PROJECT=tsconfig.test.json npx c8 --src ./src node --test test/*.js",
    "test:coverage:json": "TS_NODE_PROJECT=tsconfig.test.json npx c8 --reporter=json --src ./src node --test test/*.js",
    "test:coverage:lcov": "TS_NODE_PROJECT=tsconfig.test.json npx c8 --reporter=lcov --src ./src node --test test/*.js",
    "test:watch": "TS_NODE_PROJECT=tsconfig.test.json node --test test/*.js"
  },
  "commitlint": {
    "extends": [
      "@pob/commitlint-config"
    ]
  },
  "pob": {},
  "prettier": "@pob/root/prettier-config",
  "dependencies": {
    "@yarnpkg/core": "4.0.1",
    "semver": "7.5.4"
  },
  "devDependencies": {
    "@pob/commitlint-config": "6.3.1",
    "@pob/eslint-config": "53.0.0",
    "@pob/root": "9.1.0",
    "check-package-dependencies": "7.0.0",
    "eslint": "8.56.0"
  }
}
